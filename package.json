{
  "name": "nodejs-initial-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "commit": "npx git-cz",
    "lint": "eslint src --ext .ts --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "cd ./src/database/migrations && typeorm-ts-node-commonjs migration:create",
    "migration:run": "yarn typeorm -d ./src/database/data-source.ts migration:run",
    "migration:revert": "yarn typeorm -d ./src/database/data-source.ts migration:revert"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/multer": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  }
}
